<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CursoCompletoCSharp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CursoCompletoCSharp"
    xmlns:view="clr-namespace:CursoCompletoCSharp.View"
    Title="BarbecueCalculator"
    FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Lista de Pessoas"
        ContentTemplate="{DataTemplate view:LIstaPessoas}"
        Route="ListaPessoas" />

</Shell>
